.isatools-table .tool {
	position: relative;
	float: left;
	width: 100px;
	margin-right: 7px;
	background: #fff;
	-moz-border-radius-topleft: 10px;
	-moz-border-radius-bottomright: 10px;
	border-top-left-radius: 10px;
	border-bottom-right-radius: 10px;
}

.isatools-table {
	width: 960px;
    position: relative;
	border: 1px solid #8DC63F;
    margin-top: 150px;
	margin-bottom: 36px;
	background-color: #E4E4E4;
	padding-top: 8px;
	padding-right: 10px;
	padding-bottom: 18px;
	padding-left: 10px;
	-webkit-border-bottom-right-radius: 10px;
    -moz-border-radius-topleft: 10px;
    -moz-border-radius-bottomright: 10px;
    border-top-left-radius: 10px;
    border-bottom-right-radius: 10px;
}

.isatools-table a {
  display:block;
  text-align:center;
  -webkit-transition:opacity .2s linear;
  -moz-transition:opacity .2s linear;
  -o-transition:opacity .2s linear;
  transition:opacity .2s linear;
}

.isatools-table a:hover {
  opacity:.4;}

.isatools-table .button-p1-tiny {
	font-size: 9px;
}
.isatools-table .tip {
	position: absolute;
	top: -72px;
	left: 0;
	text-align: center;
	padding: 6px 10px 0 10px;
	line-height: 16px;
	width: 100px;
	height: 32px;
	margin-right: 10px;
	border: 1px solid #ccc;
	border-width: 1px 1px 0 1px;
	background: #fff;
}
.isatools-table .tip .point {
	position: absolute;
	left: 50.5px;
	bottom: -27px;
	margin: 0 auto;
	display: block;
	width: 19px;
	height: 13px;
}
.isatools-table .featured .tip {
	top: -54px;
}
.isatools-table .featured .tip .point {
	bottom: -26px;
	background-position: -467px -593px;
}

.isatools-table p.end, .isatools-table .featured .end {
	border-bottom: 0 none;
}
.isatools-table .tool .button-holder {
	height: 44px;
	padding-top: 16px;
}
.button-holder a {
	border: 0 none;
}
.isatools-table .button {
	width: 70px;
	margin: 0 auto;
	text-indent: 0;
}
.isatools-table p strong {
	color: #8DC63F;
}

.isatools-table .tool p {
	overflow: hidden;
	height: 60px;
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: none;
}
.isatools-table .tool p.note {
	font-size: 10px;
	line-height: 15px;
	padding-top: 10px;
	height: 115px;
}
.isatools-table .tool p.end {
	font-size: 10px;
	line-height: 15px;
	padding-top: 10px;
	height: 96px;
}

.isatools-table .col-end {
	margin-right: 0;
}
.isatools-table #pricing-features {
	width: 115px;
	background: none 0;
}
.isatools-table #pricing-features .details {
	width: 95px;
	border: 0 none;
}
.isatools-table #pricing-features p {
	text-align: right;
	font-size: 11px;
	width: 95px;
	white-space: nowrap;
	margin: 0 0 0 15px;
	color: #141414;
}
.isatools-table .details {
	display: block;
	height: 104px;
	text-align: center;
	width: 80px;
	margin: 0;
	margin-left: 0px;
	padding-top: 18px;
	font-size: 12px;
}
.isatools-table .featured {
	margin-top: -36px;
	border: 2px solid #8DC63F;
	background: #fff;
	-webkit-border-top-left-radius: 10px;
-webkit-border-bottom-right-radius: 10px;
-moz-border-radius-topleft: 10px;
-moz-border-radius-bottomright: 10px;
border-top-left-radius: 10px;
border-bottom-right-radius: 10px;
}
.isatools-table .featured .details {
	display: block;
	height: 138px;
}
.isatools-table .featured .title {
	font-size: 1.4em;
}
.isatools-table .featured .price {
	font-size: 40px;
	font-weight: bold
}
.isatools-table .title {
	text-transform: uppercase;
	font-size: 12px;
	font-weight: bold;
	color: #141414;
	display: block;
	margin-bottom: 14px;
}
.isatools-table .price {
	font-size: 32px;
	color: #141414;
	display: block;
	letter-spacing: -1.5px;
	margin-bottom: 4px;
}
.isatools-table .details .tool-info {
	font-size: 11px;
	text-transform: uppercase;
	text-align:center;
	color: #999;
	line-height:18px;
	padding: 0;
}
.isatools-table p {
	text-align: center;
	width: 80px;
	margin: 0;
	margin-left: 0px;
	line-height: 60px;
	font-size: 12px;
}

.isatools-table .tool img {
	border:none;
}